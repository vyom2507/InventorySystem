<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserController">
   <children>
      <Label layoutX="26.0" layoutY="25.0" text="User Dashboard (Read Only)">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      
      <TableView fx:id="tableProducts" layoutX="26.0" layoutY="70.0" prefHeight="250.0" prefWidth="550.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="50.0" text="ID" />
          <TableColumn fx:id="colName" prefWidth="250.0" text="Product Name" />
          <TableColumn fx:id="colQty" prefWidth="100.0" text="Quantity" />
          <TableColumn fx:id="colPrice" prefWidth="100.0" text="Price" />
        </columns>
      </TableView>
      
      <Button layoutX="524.0" layoutY="340.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>